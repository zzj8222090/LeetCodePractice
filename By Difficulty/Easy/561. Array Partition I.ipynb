    "class Solution(object):\n",
    "    def arrayPairSum(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        nums.sort()\n",
    "        s = 0\n",
    "        for i in range(len(nums)-1,-1,-2):\n",
    "            s+=nums[i-1]\n",
    "        return s"
   ]
  }
