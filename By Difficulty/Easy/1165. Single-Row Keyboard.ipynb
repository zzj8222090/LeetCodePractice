    "class Solution:\n",
    "    def calculateTime(self, keyboard: str, word: str) -> int:\n",
    "        init,total = 0,0\n",
    "        for i in word:\n",
    "            total+=abs(init-keyboard.find(i))\n",
    "            init = keyboard.find(i)\n",
    "        return total"
   ]
